@model Department
@{
    ViewData["Title"] = "Delete";
}

<h1>Delete @Model.Name Department</h1>



<form method="post" asp-action="Delete">

    <partial name="PartialViews/DetailsDeletePartialView" />

    <input type="submit" value="Delete" class="btn btn-primary mt-5" />
    <a asp-action="Index" class="btn btn-warning mt-5">Cancel</a>

</form>







