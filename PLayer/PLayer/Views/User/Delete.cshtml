@model UserVM
@{
    ViewData["Title"] = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";

}

<h1> You Want Delete this User</h1>

<form method="post" asp-action="Delete" enctype="multipart/form-data">

    <partial name="PartialViews/DetailsDeletePartialView" />


    <div class="mt-3">
        <input type="submit" value="Delete" class="btn btn-danger mt-3" />
