@model UserVM
@{
    ViewData["Title"] = "Edit";
  

}

<h1>Edit  User</h1>

<form method="post" asp-action="Edit" enctype="multipart/form-data">
  

    <div class="form-group">
        <label class="col-form-label" asp-for="Id"></label>
        <input readonly class="form-control" asp-for="Id" />

    </div>
    <div class="form-group">
        <label class="col-form-label" asp-for="FirstName"></label>
        <input class="form-control" asp-for="FirstName" />
        <span asp-validation-for="FirstName" class="text-danger"></span>
    </div>
    
    <div class="form-group">
        <label class="col-form-label" asp-for="LastName"></label>
        <input class="form-control" asp-for="LastName" />
        <span asp-validation-for="LastName" class="text-danger"></span>
    </div>




    <div class="form-group">
        <label class="col-form-label" asp-for="Email"></label>
        <input readonly class="form-control" asp-for="Email" />
        <span asp-validation-for="Email" class="text-danger"></span>
    </div>


    <div class="mt-3">
        <input type="submit" value="submit" class="btn btn-primary mt-5" asp-route-id="@Model.Id" />
        <a asp-action="Index" class="btn btn-warning mt-5">Back To List</a>
    </div>

</form>









